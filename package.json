{
    "name": "iport",
    "version": "0.17.1",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/hejny/iport"
    },
    "scripts": {
        "dev": "next dev -p 5000",
        "build": "rm -rf ./out && next build && next export && echo iport.prototyping.hejny.org > ./out/CNAME && echo > ./out/.nojekyll",
        "start": "next start",
        "lint": "next lint",
        "not-test": "echo \"Not testing now\"",
        "test": "npm run test-unit && npm run test-integration",
        "test-unit": "jest",
        "test-integration": "./node_modules/.bin/cypress run",
        "test-integration-interactive": "./node_modules/.bin/cypress open",
        "prettier": "prettier --config .prettierrc --ignore-unknown --write \"{pages,public,styles}/**/*\"",
        "prettier-commit": "git diff --exit-code && npm run prettier && git add . && git commit --allow-empty -m \"ðŸ§¹ Prettier\"",
        "preversion": "git pull && npm run prettier-commit && npm run lint && npm run not-test",
        "postversion": "npm run build && npx gh-pages --dotfiles --dist ./out --repo https://github.com/hejny/iport && git push && git push --tags"
    },
    "devDependencies": {
        "@faker-js/faker": "7.6.0",
        "@next/font": "13.1.4",
        "@total-typescript/ts-reset": "0.4.2",
        "@types/jest": "29.2.6",
        "@types/node": "18.11.18",
        "@types/react": "18.0.27",
        "@types/react-dom": "18.0.10",
        "cors": "2.8.5",
        "cypress": "12.3.0",
        "destroyable": "0.12.0",
        "eslint": "8.32.0",
        "eslint-config-next": "13.1.4",
        "express": "4.18.2",
        "gh-pages": "5.0.0",
        "jest": "29.4.0",
        "jest-environment-jsdom": "29.4.0",
        "moment": "2.29.4",
        "next": "13.1.4",
        "prettier": "2.8.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rxjs": "7.8.0",
        "socket.io": "3.1.2",
        "socket.io-client": "3.1.3",
        "spacetrim": "0.9.0",
        "ts-jest": "29.0.5",
        "ts-node": "10.9.1",
        "ts-node-dev": "2.0.0",
        "type-fest": "3.7.2",
        "typescript": "4.9.4",
        "waitasecond": "1.11.1"
    }
}
